<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.samuel.attendance.model.Params" table="params" schema="attendanceapi" >
    	
    		<id name="id" >
    			<generator class="increment" />
    		</id>
  		
    		<property name="startTime" />
    		<property name="offTime" />
    		<property name="lateTime" />
    		<property name="absenceTime" />
    		<property name="earlyLeaveTime" />
    		 			  		
    	</class>
      
    </hibernate-mapping>